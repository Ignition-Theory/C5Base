<?xml version="1.0"?>
<schema version="0.3">
	<table name="btContactDetails">
		<field name="bID" type="I"><key /><unsigned /></field>

		<field name="cardType" type="I"></field>

		<!-- Name -->
		<field name="orgName" type="X"></field>
		<field name="firstName" type="X"></field>
		<field name="midName" type="X"></field>
		<field name="lastName" type="X"></field>
		<field name="honorificName" type="X"></field>
		<field name="honorificSuffixName" type="X"></field>
		
		<field name="role" type="X"></field>
						
		<field name="telephone" type="X"></field>
		<field name="mobile" type="X"></field>
		
		<field name="email" type="X"></field>

		<!-- Address -->
		<field name="addressStreet" type="X"></field>
		<field name="addressCity" type="X"></field>
		<field name="addressCounty" type="X"></field>
		<field name="addressCode" type="X"></field>

	</table>
</schema>